{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" },
        { "src": "*.css", "use": "@vercel/node-sass" },
        { "src": "*.html", "use": "@vercel/static" }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "https://pokeapi.co/api/v2/$1"
        },
        { "src": "/(.*)", "dest": "/index.html" }
      ]
}